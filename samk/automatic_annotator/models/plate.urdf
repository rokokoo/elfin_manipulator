
<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="plate">

	<link name="cylinder">
		<inertial>
			<mass value="0.00001"/>
			<!-- cylindrical shell : Izz=m*r^2; Ixx=Iyy=1/12*m*(6*r^2+h^2) -->
			<!-- solid cylinder : Izz=(m*r^2)/2; Ixx=Iyy=1/12*m*(3*r^2+h^2) -->
			<inertia  ixx="0.0" ixy="0.00"  ixz="0.00" iyy="0.0" iyz="0.00" izz="0.0" />
		</inertial>
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder length="0.05" radius="0.3"/>
			</geometry>
		</visual>
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder length="0.05" radius="0.3"/>
			</geometry>
		</collision>
	</link>
  <gazebo reference="base_link">
    <gravity>0</gravity>
  </gazebo>
